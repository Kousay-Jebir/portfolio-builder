  "scripts": {
    "build": "nest build",
    "build:main": "nest build main-backend",
    "build:builder": "nest build builder-app",
    "build:consult": "nest build consult-app",
    "build:shared": "nest build shared",
    "start:main": "nest start main-backend",
    "start:builder": "nest start builder-app",
    "start:consult": "nest start consult-app",
    "start:dev:main": "nest start --watch main-backend",
    "start:dev:builder": "nest start --watch builder-app",
    "start:dev:consult": "nest start --watch consult-app",
    "start:all": "concurrently \"npm:start:dev:main\" \"npm:start:dev:builder\" \"npm:start:dev:consult\"",
    "test": "jest",
    "test:main": "jest apps/main-backend",
    "test:builder": "jest apps/builder-app",
    "test:consult": "jest apps/consult-app",
    "test:shared": "jest libs/shared",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"{apps,libs}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mongoose": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.0.20",
    "@nestjs/swagger": "^11.0.5",
    "@nestjs/websockets": "^11.0.20",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.7",
