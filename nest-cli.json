{
  "$schema": "https://json.schemastore.org/nest-cli",
  "monorepo": true,
  "root": ".",
  "projects": {
    "main-backend": {
      "type": "application",
      "root": "apps/main-backend",
      "entryFile": "main",
      "sourceRoot": "apps/main-backend/src"
    },
    "builder-app": {
      "type": "application",
      "root": "apps/builder-app",
      "entryFile": "main",
      "sourceRoot": "apps/builder-app/src"
    },
    "consult-app": {
      "type": "application",
      "root": "apps/consult-app",
      "entryFile": "main",
      "sourceRoot": "apps/consult-app/src"
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src"
    }
  }
}